#define FIXED_RECT_FROM_LEFT( rectName, rel_x, rel_y, MinWidth, colorValue, visibleValue) \
			part { \
				name: rectName; \
				type: RECT; \
				mouse_events: 0; \
				scale: 1; \
				description { \
					state: "default" 0.0; \
					min: MinWidth 0; \
					fixed: 1 1; \
					rel1 { relative: 1.0 0.0; to_x: rel_x; to_y: rel_y; } \
					rel2 { relative: 1.0 1.0; to_x: rel_x; to_y: rel_y; } \
					align: 0.0 0.5; \
					color : colorValue; \
					visible: visibleValue; \
				} \
				description { \
					state: "hide" 0.0; \
					inherit: "default" 0.0; \
					min: 0 0; \
					visible: 0; \
				} \
			} \

