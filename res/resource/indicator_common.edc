#define TEMP_SWALLOW_PART_FROM_LEFT( partName, rel_x, rel_y, MinWidth, MinHeight) \
			part { \
				name: partName; \
				type: SWALLOW; \
				mouse_events: 0; \
				scale: 1; \
				description { \
					state: "default" 0.0; \
					min: MinWidth MinHeight; \
					fixed: 1 0; \
					rel1 { relative: 1.0 0.0; to_x: rel_x; to_y: rel_y; } \
					rel2 { relative: 1.0 1.0; to_x: rel_x; to_y: rel_y; } \
					align: 0.5 0.5; \
					visible: 1; \
				} \
			} \

